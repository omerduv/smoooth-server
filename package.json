{
  "name": "smoooth-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "nodemon ./src/index.js --exec babel-node",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "build2": "babel src --out-dir dist --copy-files",
    "start": "node ./index.js",
    "test": "eslint src"
  },
  "author": "",
  "license": "ISC",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "express": "^4.15.4",
    "mongo": "^0.1.0",
    "request": "^2.81.0",
    "with-query": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.6.1",
    "nodemon": "^1.12.0"
  }
}
